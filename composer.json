{
    "name": "site/site-core",
    "type": "typo3-cms-extension",
    "description": "Core Extension for a TYPO3 Application.",
    "license": "GPL-2.0-or-later",
    "version": "1.5.1",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Mati",
            "email": "mati_01@icloud.com",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "10.4.14",
        "symfony/dotenv": "*"
    },
    "autoload": {
        "psr-4": {
            "Site\\Core\\": "Classes/"
        }
    },
    "suggest": {
        "site/site-backend": "Should be used in combination with core's TCA-Service to build incredible-fast Content-Elements or configure backend with a clean API.",
        "site/site-frontend": "Same as above just for your frontend."
    },
    "scripts": {
        "post-autoload-dump": [
            "Site\\Core\\Composer\\EnvLoader::postAutoloadDump"
        ]
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "site_core"
        }
    }
}
