{
    "name": "site/site-core",
    "type": "typo3-cms-extension",
    "description": "Core Extension for any modern TYPO3 Application.",
    "license": "GPL-2.0-or-later",
    "version": "v4.0.0",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Mati",
            "email": "mati_01@icloud.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8",
        "typo3/cms-core": "^11.5 || ^12",
        "symfony/dotenv": "^5.0 || ^6.0",
        "dflydev/dot-access-data": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Site\\Core\\": "Classes/"
        }
    },
    "suggest": {
        "site/site-frontend": "site/site-core depends on site/site-frontend.",
        "site/site-backend": "site/site-core heavily depends on site/site-backend."
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "site_core"
        }
    }
}
